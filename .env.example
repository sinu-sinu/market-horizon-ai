  # OpenAI
  OPENAI_API_KEY=sk-your-key-here
  
  # Serper.dev (free tier: 2500/month)
  SERPER_API_KEY=your-serper-key
  
  # Reddit (optional)
  REDDIT_CLIENT_ID=your-client-id
  REDDIT_CLIENT_SECRET=your-client-secret
  REDDIT_USER_AGENT=MarketHorizonAI/1.0
  
  # YouTube (optional)
  YOUTUBE_API_KEY=your-youtube-key
  
  # Langfuse Observability (self-hosted v2)
  # 1. Start Langfuse: docker-compose -f docker/docker-compose.langfuse.yml up -d
  # 2. Open http://localhost:3000 and create an account
  # 3. Create a project and get API keys from Settings > API Keys
  # 4. Update the keys below with your actual keys
  LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
  LANGFUSE_SECRET_KEY=sk-lf-your-secret-key
  LANGFUSE_HOST=http://localhost:3000
  LANGFUSE_ENABLED=true

  # Application Settings
  APP_ENV=development
  LOG_LEVEL=INFO
  CACHE_TTL_DAYS=7
  MAX_TOKENS_PER_QUERY=15000
  BUDGET_LIMIT_TOKENS=100000